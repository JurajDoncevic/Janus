{
  "ComponentOptions": {
    "NodeId": "DebugCsvWrapperNode1",
    "ListenPort": 20001,
    "TimeoutMs": 5000,
    "DataSourcePath": "./DataSet",
    "StartupRemotePoints": [
      {
        "NodeType": "MASK",
        "Address": "127.0.0.1",
        "ListenPort": 10001
      },
      {
        "NodeType": "WRAPPER",
        "Address": "127.0.0.1",
        "ListenPort": 30001
      },
      {
        "NodeType": "MEDIATOR",
        "Address": "127.0.0.1",
        "ListenPort": 20002
      }
    ]
  },

  "NLog": {
    "targets": {
      "file": {
        "type": "File",
        "fileName": "./log-${shortdate}.log",
        "layout": "${longdate}|${logger}|${message}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "file"
      }
    ]
  }
}
