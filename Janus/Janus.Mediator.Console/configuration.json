{
  "ComponentOptions": {
    "NodeId": "DebugMediatorNode",
    "ListenPort": 20001,
    "TimeoutMs": 5000,
    "StartupRemotePoints": [
      {
        "NodeType": "MASK",
        "Address": "127.0.0.1",
        "Port": "10001"
      },
      {
        "NodeType": "WRAPPER",
        "Address": "127.0.0.1",
        "Port": "30001"
      }
    ]
  },

  "NLog": {
    "targets": {
      "file": {
        "type": "File",
        "fileName": "./log-${shortdate}.log",
        "layout": "${longdate}|${logger}|${message}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "file"
      }
    ]
  }
}
